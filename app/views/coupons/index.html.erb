<h1>Coupons</h1>

<!-- < link_to 'Write New Article!!', new_article_path %> -->

<table>
    <tr>
        <th>Coupon Code</th>
        <th>Store</th>
        <th>Link</th>
    </tr>

    <% @coupons.each do |coupon| %>
        <tr>
            <td><%= coupon.coupon_code %></td>
            <td><%= coupon.store %></td>
            <!-- <td>< link_to 'Show', coupon_path(coupon) %></td> -->
            <!-- <td><  link_to coupon.store, coupon_path(coupon) %></td> -->
            <td><%= link_to coupon.coupon_code, coupon_path(coupon) %></td>
        </tr>
    <% end %>
</table>
